// status: correct

loadString("
model foo
  function f
    input Real x;
    output Real y;
    output Real y1;
  protected
    Real a[2];
  algorithm
     a[1] := 1;
     a[2] := 1;
     y := sum(a)*x;
     y1 := sum(a)*x + y;
    annotation(Inline=true); 
  end f;

  Real y(start=0,fixed=true);
  Real dy = der(y);
equation
 der(y) + 1 = f(der(y));
end foo;
");
getErrorString();
setCommandLineOptions("+d=dumpBackendInline");
getErrorString();

simulate(foo);
getErrorString();

val(time,{0,0.5,1.0});
val(y,{0,0.5,1.0});
