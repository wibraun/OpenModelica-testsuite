// name: testADOLC9
// status: correct
// teardown_command: rm -f adolcTest* output.log

// function with more then one output

loadString("
  model adolcTest
    Real x(start=1.0, fixed=true);
    Real y, z, w;
    function f
      input Real x;
      output Real y;
      output Real z;
      output Real w;
    protected 
      //Real w = sin(3.14);
    algorithm
      y := x*sin(x);
      z := x+y;
      w := z+x;
    end f;
  equation
    (y,z,w) = f(x);
    der(x) = y+time*z-w;
  end adolcTest;
");
getErrorString();


//setCommandLineOptions("--adolcTrace --tearingMethod=noTearing");
//getErrorString();
setCommandLineOptions("--adolcTrace");
getErrorString();


simulate(adolcTest, simflags="-jacobian=adolc");
getErrorString();
val(x, 1.0);


simulate(adolcTest, simflags="-jacobian=adolcSparse");
getErrorString();
val(x, 1.0);

simulate(adolcTest);
getErrorString();
val(x, 1.0);


